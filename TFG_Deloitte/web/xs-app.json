{
  "welcomeFile": "/web/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/web/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },{
	  "source": "(.*)(.xsjs)",
	  "destination": "core-xsjs-backend"
	},{
	  "source": "(.*)(.xsodata)",
	  "destination": "core-xsjs-backend"
	}
  ]
}