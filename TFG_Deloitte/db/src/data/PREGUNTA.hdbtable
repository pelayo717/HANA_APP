COLUMN TABLE "PREGUNTA" (
	"ID_PREGUNTA" INTEGER NOT NULL GENERATED BY DEFAULT AS IDENTITY COMMENT 'IDENTIFICADOR DE LA PREGUNTA',
	"TIPO" NVARCHAR(10) NOT NULL COMMENT 'TIPO DE LA PREGUNTA',
	"DESCRIPCION" NVARCHAR(150) NOT NULL COMMENT 'DESCRIPCION DE LA PROPIA PREGUNTA',
	PRIMARY KEY("ID_PREGUNTA")
)

WITH ASSOCIATIONS( 
	JOIN "RESPUESTA" AS "RESPUESTAS" ON "ID_PREGUNTA" = "ID_PREGUNTA"
)