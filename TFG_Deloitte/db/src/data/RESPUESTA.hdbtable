COLUMN TABLE "RESPUESTA" (
	"ID_PREGUNTA" INTEGER NOT NULL COMMENT 'IDENTIFICADOR DE LA PREGUNTA (REFERENCIA)',
	"NUM_RESPUESTA" INTEGER NOT NULL COMMENT 'NUMERO DE LA POSIBLE RESPUESTA A LA PREGUNTA',
	"NOMBRE_RESPUESTA" NVARCHAR(20) NOT NULL COMMENT 'POSIBLE VALOR DE LA RESPUESTA A LA PREGUNTA',
	"VALOR_RESPUESTA" INTEGER NOT NULL COMMENT 'VALOR DE LA RESPUESTA A LA PREGUNTA',
	PRIMARY KEY ("ID_PREGUNTA","NUM_RESPUESTA")
)

WITH ASSOCIATIONS( JOIN "PREGUNTA" AS "PREGUNTA" ON "PREGUNTA"."ID_PREGUNTA" = "ID_PREGUNTA")
