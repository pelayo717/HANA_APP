COLUMN TABLE "PREGUNTA_CUESTIONARIO" (
	"ID_CUESTIONARIO" INTEGER NOT NULL COMMENT 'IDENTIFICADOR DEL CUESTIONARIO (REFERENCIA)',
	"ID_PREGUNTA" INTEGER NOT NULL COMMENT 'IDENTIFICADOR DE LA PREGUNTA (REFERENCIA)',
	PRIMARY KEY("ID_CUESTIONARIO","ID_PREGUNTA")
)

WITH ASSOCIATIONS (
	JOIN "CUESTIONARIO" AS "CUESTIONARIO" ON "CUESTIONARIO"."ID_CUESTIONARIO" = "ID_CUESTIONARIO",
	JOIN "PREGUNTA" AS "PREGUNTA" ON "PREGUNTA"."ID_PREGUNTA" = "ID_PREGUNTA"
)