PROCEDURE "OBTENERRIESGOEVALUACIONES" (IDENTIFICADOR_RIESGO INTEGER) 
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER 
AS 
BEGIN 
   SELECT RIESGO.ID_RIESGO,
		EVALUACION.NUM_EVALUACION,
		EVALUACION.FECHA_EVALUACION,
		EVALUACION.PROBABILIDAD_FINAL,
		EVALUACION.IMPACTO_FINAL,
		EVALUACION.NIVEL_RIESGO_APLICADO
   FROM  RIESGO INNER JOIN EVALUACION ON EVALUACION.ID_RIESGO = RIESGO.ID_RIESGO
   WHERE RIESGO.ID_RIESGO = IDENTIFICADOR_RIESGO;
END