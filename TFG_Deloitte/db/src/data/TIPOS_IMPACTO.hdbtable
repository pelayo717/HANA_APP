COLUMN TABLE "TIPOS_IMPACTO" (
	"ID_RIESGO" INTEGER NOT NULL COMMENT 'IDENTIFICADOR DEL RIESGO (REFERENCIA)',
	"TIPO_IMPACTO" NVARCHAR(20) NOT NULL COMMENT 'TIPO DE IMPACTO (REFERENCIA)',
	PRIMARY KEY ("ID_RIESGO","TIPO_IMPACTO")
)

WITH ASSOCIATIONS( JOIN "RIESGO" AS "RIESGO" ON "RIESGO"."ID_RIESGO" = "ID_RIESGO")

